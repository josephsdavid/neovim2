{
  "Function Declaration": {
    "prefix": "function",
    "body": [
      "function ${1:name}${2}",
      "\t$0",
      "end  # function ${1:name}"
    ],
    "description": "Function declaration with ending comment"
  },
  "Macro Declaration": {
    "prefix": "macro",
    "body": [
      "macro ${1:name}${2}",
      "\t$0",
      "end  # macro ${1:name}"
    ],
    "description": "Macro declaration with ending comment"
  },

  "Module":{ 
    "prefix": "module",
    "body": [
    "module ${1:name}",
    "${2}",
    "end  # module ${1}"
    ]
  },
  "Using":{
      "prefix": "using",
      "body": "using ${1:module}"
  },
  "Include":{ 
    "prefix": "include",
    "body": "include('${1:file}')"
  },
  "Typed variable":{
      "prefix": "tv",
      "body": "${1:var}::${2:type}"
      },
  "For/Iterable":{
      "prefix": "for",
      "body": [
        "for ${1:item} in ${2:iterable}",
        "\t${3:print}(${1})",
        "end"
      ]},
  "For/Sized":{
      "prefix": "fors",
      "body": [
        "let",
        "\t${6}${1:xx} = ${2:0.0}",
        "\tfor ${3:i} = ${4:1}:${5:1000}",
        "\t\t${1} += ${3}",
        "\tend",
        "\t$1",
        "end"
      ]
      }
}
